	Reset1,
	SENSOR_LINEA,
	SENSOR_MURO,
	sampler_tx
);
input  ADC_DOUT;
input  CLK;
input  Reset1;
input  SENSOR_LINEA;
input  SENSOR_MURO;
output sampler_tx;

reg sample;
time current_time;
always @(ADC_DOUT or CLK or Reset1 or SENSOR_LINEA or SENSOR_MURO)
                                                                                
begin                                                 
 if ($realtime > 0)                                   
