	.ADC_DIN(ADC_DIN),
	.ADC_DOUT(ADC_DOUT),
	.ADC_SCLK(ADC_SCLK),
	.C_ad(C_ad),
	.C_at(C_at),
	.C_der(C_der),
	.C_izq(C_izq),
	.CLK(CLK),
	.CUENTAA(CUENTAA),
	.GIROO(GIROO),
	.MD0(MD0),
	.MD1(MD1),
	.MI0(MI0),
	.MI1(MI1),
	.MUROO(MUROO),
	.P_ad(P_ad),
