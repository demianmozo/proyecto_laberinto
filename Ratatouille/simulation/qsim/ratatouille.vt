);

// CLK
always
begin
	CLK = 1'b0;
	CLK = #10000 1'b1;
	#10000;
end 

// Reset1
initial
begin
	Reset1 = 1'b0;
	Reset1 = #4000000 1'b1;
end 

// SENSOR_LINEA
initial
begin
	SENSOR_LINEA = 1'b1;
	SENSOR_LINEA = #3200000 1'b0;
	# 800000;
	repeat(4)
	begin
		SENSOR_LINEA = 1'b1;
		SENSOR_LINEA = #19200000 1'b0;
		# 800000;
	end
	SENSOR_LINEA = 1'b1;
end 

// SENSOR_MURO
initial
begin
	SENSOR_MURO = 1'b1;
end 

Matriz_ubicacion_vlg_sample_tst tb_sample (
	.ADC_DOUT(ADC_DOUT),
	.CLK(CLK),
	.Reset1(Reset1),
	.SENSOR_LINEA(SENSOR_LINEA),
	.SENSOR_MURO(SENSOR_MURO),
	.sampler_tx(sampler)
);

Matriz_ubicacion_vlg_check_tst tb_out(
	.ADC_CS_N(ADC_CS_N),
	.ADC_DIN(ADC_DIN),
	.ADC_SCLK(ADC_SCLK),
	.C_ad(C_ad),
	.C_at(C_at),
	.C_der(C_der),
	.C_izq(C_izq),
	.CUENTAA(CUENTAA),
	.GIROO(GIROO),
	.MD0(MD0),
	.MD1(MD1),
	.MI0(MI0),
	.MI1(MI1),
	.MUROO(MUROO),
	.P_ad(P_ad),
	.P_at(P_at),
	.P_der(P_der),
	.P_izq(P_izq),
	.pos_act(pos_act),
	.sent_act(sent_act),
	.SENTIDOO(SENTIDOO),
	.VELD(VELD),
	.VELI(VELI),
	.sampler_rx(sampler)
);
endmodule

